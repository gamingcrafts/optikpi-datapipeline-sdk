{
    "name": "optikpi/datapipeline-sdk",
    "description": "Official PHP SDK for Optikpi Data Pipeline API",
    "type": "library",
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Optikpi",
            "email": "support@optikpi.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-openssl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Optikpi\\DataPipeline\\": "src/Optikpi/DataPipeline/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Optikpi\\DataPipeline\\Tests\\": "tests/"
        }
    },
    "keywords": [
        "optikpi",
        "datapipeline",
        "api",
        "sdk",
        "client",
        "php",
        "hmac",
        "hkdf",
        "authentication"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/gamingcrafts/optikpi-datapipeline-sdk.git"
    },
    "support": {
        "issues": "https://github.com/gamingcrafts/optikpi-datapipeline-sdk/issues"
    }
}